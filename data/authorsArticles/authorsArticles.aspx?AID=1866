<script type="text/javascript">
    var strcountry
    var strhost
    function GetUserInfo(data) {
        strcountry = data.country;
        strhost = data.ip;
    }
    $(function () {
        BindUserInfo();
    })
    function BindUserInfo() {
        $.post("magazineMasterPage.master", { Country: strcountry, UserIP: strhost });
    }
</script>
