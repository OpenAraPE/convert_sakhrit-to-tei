            $('#nav > div').hover(
                function () {
                    var $this = $(this);
                    $this.find('img').stop().animate({
                        'width': '59px',
                        'height': '59px',
                        'top': '-10px',
                        'right': '-5px',
                        'opacity': '1.0'
                    }, 500, 'easeOutBack', function () {
                        //$(this).parent().find('ul').fadeIn(600);
                    });

                    //$(this).parent().find('ul').fadeIn(600); (Tablet Mode size)
                    //$(this).parent().find('ul').fadeIn(700); (default mode size)

                    $this.find('a:first,h2').addClass('active');
                },
                function () {
                    var $this = $(this);
                    $this.find('ul').fadeOut(980);
                    $this.find('img').stop().animate({
                        'width': '24px',
                        'height': '24px',
                        'top': '0px',
                        'right': '0px',
                        'opacity': '0.1'
                    }, 5000, 'easeOutBack');

                    $this.find('a:first,h2').removeClass('active');
                }
            );
        });
