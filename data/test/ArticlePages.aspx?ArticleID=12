                        function tw_click() {
                            url = location.href; text = 'أرشيف_المجلات_الأدبية_و_الثقافية_العربية#'; via = 'Sakhrit';
                            window.open('https://twitter.com/share?url=' + encodeURIComponent(url) + '&text=' + encodeURIComponent(text) + '&via=' + encodeURIComponent(via), 'share', 'toolbar=0,status=0,width=626,height=436');
                            return false;
                        }
